/* Old: based on golden ratio
$size-m4:       4px;
$size-m3:       7px;
$size-m2:       9px;
$size-m1:       12px; // Minus Sizes [m]
$size-smaller:  14px; // dumb but super useful
$size-base:     $size; 
$size-larger:   19px; // dumb but super useful
$size-p1:       21px; // Plus Sizes [p]
$size-p2:       28px;
$size-p3:       37px;
$size-p4:       50px;
$size-p5:       67px;
$size-p6:       89px;
$size-p7:       119px;
$size-p8:       159px;

$size-half:     $size/2;
$size-2:        $size*2;
*/
