._grid_item-overflow, ._wireframe-desc, ._wireframe-title, ._wf-title, ._wf-desc, ._wireframe-comments, ._wf-comments {
  /*
    https://stackoverflow.com/questions/43311943/prevent-content-from-expanding-grid-items

    By default, a grid item cannot be smaller than the size of its content.
    Grid items have an initial size of min-width: auto and min-height: auto.
    You can override this behavior with min-width: 0, min-height: 0 or overflow with any value other than visible.
  */ }
  ._grid_item-overflow, ._wireframe-desc, ._wireframe-title, ._wf-title, ._wf-desc, ._wireframe-comments, ._wf-comments, ._grid_item-overflow-hidden {
    overflow: hidden; }
  ._grid_item-overflow-scroll {
    overflow: scroll; }
  ._grid_item-overflow, ._wireframe-desc, ._wireframe-title, ._wf-title, ._wf-desc, ._wireframe-comments, ._wf-comments, ._grid_item-overflow *, ._wireframe-desc *, ._wireframe-title *, ._wf-title *, ._wf-desc *, ._wireframe-comments *, ._wf-comments * {
    min-height: 0;
    /* NEW; needed for Firefox */
    min-width: 0;
    /* NEW; needed for Firefox */ }

