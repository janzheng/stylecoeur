/*
    Pattern

    _margin
    _margin-half
    _margin-none
    _margin-top
      _margin-top-i
      _margin-top-half
        _margin-top-half-i
      _margin-top-2
        _margin-top-2-i
    _margin-bottom
    _margin-left
    _margin-right

    modifier:
    -none
    -half
    -2
    -i
    -xs (only xs)
    -sm (sm and up)
    -md (md and up)
    -lg (lg and up)
*/
/*

  ._margin {
    @include expanderer('margin');
  }

  results in _margin-i, _margin-half, _margin-half-i, etc.

*/
