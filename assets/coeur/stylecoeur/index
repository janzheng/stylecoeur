  zoom: 1; }

/*
    Keep in mind:

    Media Queries will bubble and nest:
        p {
          @media query-1 {
            first thing
            @media query-2 {
              second thing
            }
          }
        }

        will result in CSS:

        @media query-1 {
          p {
            first thing
          }
        }
        @media query-1 and query-2 {
          p {
            second thing
          }
        }


*/
/*
  // can take an array like ('xs', 'sm', 'md')

  ._center {
    text-align: center;

    @include genGridAttr(('xs') { 
      text-align: right;
    }
  }

  result:

  @media xs query {
    ._center-xs {
      text-align: right
    }
  }
*/
/* usage:

  ._hidden {
    display: none;

    @include genGridAttr(('xs','sm','sm-down','md','lg')) {
      display: none;
    }
  }
*/
