  @media (min-width: 768px) {
    ._flex-sm {
      display: flex; } }
  @media (min-width: 0px) and (max-width: 991px) {
    ._flex-sm-down {
      display: flex; } }
  @media (min-width: 992px) {
    ._flex-md {
      display: flex; } }
  @media (min-width: 1200px) {
    ._flex-lg {
      display: flex; } }

